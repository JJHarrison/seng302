<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.*?>
<ToolBar xmlns:fx="http://javafx.com/fxml/1" prefHeight="40.0" prefWidth="200.0" xmlns="http://javafx.com/javafx/8">
  <items>
      <Button fx:id="loadButton" mnemonicParsing="false" text="Load" />
      <Button fx:id="saveButton" mnemonicParsing="false" text="Save" />
      <Separator orientation="VERTICAL">
         <padding>
            <Insets left="10.0" right="10.0" />
         </padding>
      </Separator>
      <Button fx:id="undoButton" mnemonicParsing="false" text="Undo" />
      <Button fx:id="redoButton" mnemonicParsing="false" text="Redo" />
      <Separator orientation="VERTICAL">
         <padding>
            <Insets left="10.0" right="10.0" />
         </padding>
      </Separator>
      <Button fx:id="reportButton" mnemonicParsing="false" text="Report" />
  </items>
</ToolBar>
