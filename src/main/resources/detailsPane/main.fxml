<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.StackPane?>
<BorderPane fx:id="detailsPane" xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/2.2" fx:controller="seng302.group4.viewModel.DetailsPaneController">
  <bottom>
    <Button fx:id="editButton" defaultButton="true" mnemonicParsing="false" text="Edit" BorderPane.alignment="CENTER_RIGHT" />
  </bottom>
  <center>
    <StackPane fx:id="stackPane">
      <children>
        <fx:include fx:id="projectDetailsPane" source="project.fxml" />
        <fx:include fx:id="personDetailsPane" source="person.fxml" />
        <fx:include fx:id="skillDetailsPane" source="skill.fxml" />
        <fx:include fx:id="teamDetailsPane" source="team.fxml" />
      </children>
    </StackPane>
  </center>
</BorderPane>
