<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.Font?>
<BorderPane xmlns:fx="http://javafx.com/fxml/1" fx:id="mainBorderPane" maxHeight="-Infinity" maxWidth="-Infinity" minWidth="-Infinity" prefHeight="550.0" prefWidth="1000.0" stylesheets="css/styles.css" xmlns="http://javafx.com/javafx/2.2" fx:controller="com.thirstygoat.kiqo.viewModel.MainController">
  <center>
    <SplitPane fx:id="mainSplitPane" dividerPositions="0.31" maxHeight="-1.0" minHeight="-1.0" prefWidth="200.0" BorderPane.alignment="CENTER">
      <items>
        <VBox fx:id="listPane" opacity="1.0" spacing="10.0">
          <children>
            <Label fx:id="listLabel" alignment="CENTER_LEFT" maxHeight="-1.0" maxWidth="-1.0" prefHeight="-1.0" prefWidth="-1.0" styleClass="title" text="project">
              <font>
                <Font name="System Bold" size="18.0" />
              </font>
              <VBox.margin>
                <Insets left="10.0" right="10.0" top="10.0" />
              </VBox.margin>
            </Label>
            <TabPane fx:id="tabViewPane" VBox.vgrow="ALWAYS">
              <tabs>
                <Tab fx:id="projectTab" closable="false" text="Project">
                  <content>
                    <ListView fx:id="projectListView" editable="true" />
                  </content>
                </Tab>
                <Tab fx:id="teamsTab" closable="false" text="Teams">
                  <content>
                    <ListView fx:id="teamsListView" />
                  </content>
                </Tab>
                <Tab fx:id="peopleTab" closable="false" text="People">
                  <content>
                    <ListView fx:id="peopleListView" />
                  </content>
                </Tab>
                <Tab fx:id="skillsTab" closable="false" text="Skills">
                  <content>
                    <ListView fx:id="skillsListView" />
                  </content>
                </Tab>
                <Tab fx:id="releasesTab" closable="false" text="Release">
                  <content>
                    <ListView fx:id="releasesListView" />
                  </content>
                </Tab>
              </tabs>
            </TabPane>
          </children>
        </VBox>
        <fx:include fx:id="detailsPane" source="detailsPane/main.fxml" />
      </items>
    </SplitPane>
  </center>
  <top>
    <fx:include fx:id="menuBar" source="menubar.fxml" />
  </top>
</BorderPane>
